
#include <bits/stdc++.h>

using namespace std;

#define MAX 1000

class Stack {
	int top;

public:
	int arr[MAX]; // Maximum size of Stack

	Stack() { top = -1; }
	
bool push(int value)
{
	if (top >= (MAX - 1)) {
		cout << "Stack Overflow";
		return false;
	}
	else {
		arr[++top] = value;
		cout << value << " pushed into stack\n";
		return true;
	}
}

int pop()
{
	if (top < 0) {
		cout << "Stack Underflow";
		return 0;
	}
	else {
		int val = arr[top--];
		return val;
	}
}
int peek()
{
	if (top < 0) {
		cout << "Stack is Empty";
		return 0;
	}
	else {
		int val = arr[top];
		return val;
	}
}

bool isEmpty()
{
	return (top < 0);
}
};

int main()
{
	class Stack s;
	s.push(10);
	s.push(20);
	s.push(30);
	cout << s.pop() << " Popped from stack\n";
	
	cout<<"Elements present in stack : ";
	while(!s.isEmpty())
	{
		cout<<s.peek()<<" ";
		s.pop();
	}

	return 0;
}
